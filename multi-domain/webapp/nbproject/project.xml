<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.web.project</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/web-project/3">
            <name>MultiDomain-Web-App</name>
            <minimum-ant-version>1.6</minimum-ant-version>
            <web-module-libraries>
                <library files="1">
                    <file>${file.reference.net.java.hulp.i18n.jar}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library files="1">
                    <file>${file.reference.dwr.jar}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library files="2">
                    <file>${libs.jstl11.classpath}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library files="2">
                    <file>${libs.spring-framework.classpath}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library files="1">
                    <file>${libs.spring-webmvc.classpath}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library files="1">
                    <file>${file.reference.index-core.jar}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library files="1">
                    <file>${reference.MultiDomain-Core-Services.jar}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library files="1">
                    <file>${reference.MultiDomain-Web-Tier-Services.jar}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
            </web-module-libraries>
            <web-module-additional-libraries/>
            <source-roots>
                <root id="src.dir" name="MultiDomain Web App Source Package"/>
            </source-roots>
            <test-roots>
                <root id="test.src.dir"/>
            </test-roots>
        </data>
        <references xmlns="http://www.netbeans.org/ns/ant-project-references/1">
            <reference>
                <foreign-project>MultiDomain-Core-Services</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar</id>
            </reference>
            <reference>
                <foreign-project>MultiDomain-Web-Tier-Services</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar</id>
            </reference>
        </references>
        <spring-data xmlns="http://www.netbeans.org/ns/spring-data/1">
            <config-files>
                <config-file>web/WEB-INF/applicationContext.xml</config-file>
                <config-file>web/WEB-INF/dispatcher-servlet.xml</config-file>
                <config-file>web/WEB-INF/jsp/index.jsp</config-file>
            </config-files>
            <config-file-groups>
                <config-file-group name="Default Group">
                    <config-file>web/WEB-INF/applicationContext.xml</config-file>
                    <config-file>web/WEB-INF/dispatcher-servlet.xml</config-file>
                    <config-file>web/WEB-INF/jsp/index.jsp</config-file>
                </config-file-group>
            </config-file-groups>
        </spring-data>
    </configuration>
</project>
