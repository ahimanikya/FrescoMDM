<?xml-stylesheet type="text/xml" href="http://openide.netbeans.org/fs/filesystem.xsl"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="Templates">
        <folder name="Project">        
            <folder name="CAPS">
                <folder name="MDM">
                    <attr name="position" intvalue="2000"/>
                    <attr name="SystemFileSystem.localizingBundle" stringvalue="com.sun.mdm.index.project.ui.wizards.Bundle"/>
                    <attr name="instantiatingWizardURL" urlvalue="nbresloc:/com/sun/mdm/index/project/ui/resources/project.html"/>

                    <!-- Add template for the eView enterprize project -->
                    <file name="Master Index Application">
                        <attr name="template" boolvalue="true"/>
                        <attr name="templateWizardIterator" newvalue="com.sun.mdm.index.project.ui.wizards.EVIEWIterator"/>
                        <attr name="SystemFileSystem.localizingBundle" stringvalue="com.sun.mdm.index.project.ui.wizards.Bundle"/>
                        <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/com/sun/mdm/index/project/ui/resources/eviewproProjectIcon.png"/>
                        <attr name="templateWizardURL" urlvalue="nbresloc:/com/sun/mdm/index/project/ui/resources/emptyProject.html"/>
                    </file>
                </folder>
            </folder>
        </folder>
    </folder>
    <folder name="com-sun-mdm-index-project">
        <file name="eviewmap.xml" url="eviewmap.xml"/>
        <file name="genPortmap.xsl" url="genPortmap.xsl"/>
        <file name="untitled.eview" url="untitled.eview"/>
    </folder>
</filesystem>
