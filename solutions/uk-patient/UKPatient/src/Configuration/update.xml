<?xml version="1.0" encoding="UTF-8"?><Configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schema/update.xsd">    <SurvivorHelperConfig module-name="SurvivorHelper" parser-class="com.sun.mdm.index.configurator.impl.SurvivorHelperConfig">        <helper-class>com.sun.mdm.index.survivor.impl.DefaultSurvivorHelper</helper-class>        <default-survivor-strategy>            <strategy-class>com.sun.mdm.index.survivor.impl.WeightedSurvivorStrategy</strategy-class>            <parameters>                <parameter>                    <parameter-name>ConfigurationModuleName</parameter-name>                    <parameter-type>java.lang.String</parameter-type>                    <parameter-value>WeightedSurvivorCalculator</parameter-value>                </parameter>            </parameters>        </default-survivor-strategy>        <candidate-definitions>            <candidate-field name="UKPatient.Category"/>            <candidate-field name="UKPatient.Gender"/>            <candidate-field name="UKPatient.DOB"/>            <candidate-field name="UKPatient.BirthOrder"/>            <candidate-field name="UKPatient.BirthTime"/>            <candidate-field name="UKPatient.MaritalStatus"/>            <candidate-field name="UKPatient.DateOfDeath"/>            <candidate-field name="UKPatient.StatusOfDeathNoti"/>            <candidate-field name="UKPatient.Language"/>            <candidate-field name="UKPatient.EthnicCatCode"/>            <candidate-field name="UKPatient.CommonContactMeth"/>            <candidate-field name="UKPatient.ContactPref01STime"/>            <candidate-field name="UKPatient.ContactPref01ETime"/>            <candidate-field name="UKPatient.ContactPref02STime"/>            <candidate-field name="UKPatient.ContactPref02ETime"/>            <candidate-field name="UKPatient.ContactPref03STime"/>            <candidate-field name="UKPatient.ContactPref03ETime"/>            <candidate-field name="UKPatient.CommentTxt"/>            <candidate-field name="UKPatient.Name[*].*"/>            <candidate-field name="UKPatient.Address[*].*"/>            <candidate-field name="UKPatient.Telecom[*].*"/>            <candidate-field name="UKPatient.Consent[*].*"/>            <candidate-field name="UKPatient.AltContact[*].*"/>            <candidate-field name="UKPatient.AltConTele[*].*"/>            <candidate-field name="UKPatient.AuxID[*].*"/>            <candidate-field name="UKPatient.HeightWeight[*].*"/>            <candidate-field name="UKPatient.HealthID[*].*"/>        </candidate-definitions>    </SurvivorHelperConfig>    <WeightedCalculator module-name="WeightedSurvivorCalculator" parser-class="com.sun.mdm.index.configurator.impl.WeightedCalculatorConfig">        <default-parameters>            <parameter>                <quality>SourceSystem</quality>                <preference>SUN</preference>                <utility>100.0</utility>            </parameter>            <parameter>                <quality>MostRecentModified</quality>                <utility>75.0</utility>            </parameter>        </default-parameters>    </WeightedCalculator>    <UpdateManagerConfig module-name="UpdateManager" parser-class="com.sun.mdm.index.configurator.impl.UpdateManagerConfig">        <EnterpriseMergePolicy></EnterpriseMergePolicy>        <EnterpriseUnmergePolicy></EnterpriseUnmergePolicy>        <EnterpriseUpdatePolicy></EnterpriseUpdatePolicy>        <EnterpriseCreatePolicy></EnterpriseCreatePolicy>        <SystemMergePolicy></SystemMergePolicy>        <SystemUnmergePolicy></SystemUnmergePolicy>        <UndoAssumeMatchPolicy></UndoAssumeMatchPolicy>        <SkipUpdateIfNoChange>true</SkipUpdateIfNoChange>    </UpdateManagerConfig></Configuration>