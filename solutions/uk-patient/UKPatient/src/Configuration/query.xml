<?xml version="1.0" encoding="UTF-8"?><Configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schema/query.xsd">    <QueryBuilderConfig module-name="QueryBuilder" parser-class="com.sun.mdm.index.configurator.impl.querybuilder.QueryBuilderConfiguration">        <query-builder name="ALPHA-SEARCH" class="com.sun.mdm.index.querybuilder.BasicQueryBuilder" parser-class="com.sun.mdm.index.configurator.impl.querybuilder.KeyValueConfiguration" standardize="false" phoneticize="false">            <config>                <option key="UseWildcard" value="true"/>            </config>        </query-builder>        <query-builder name="PHONETIC-SEARCH" class="com.sun.mdm.index.querybuilder.BasicQueryBuilder" parser-class="com.sun.mdm.index.configurator.impl.querybuilder.KeyValueConfiguration" standardize="true" phoneticize="true">            <config>                <option key="UseWildcard" value="false"/>            </config>        </query-builder>        <query-builder name="BLOCKER-SEARCH" class="com.sun.mdm.index.querybuilder.BlockerQueryBuilder" parser-class="com.sun.mdm.index.configurator.impl.blocker.BlockerConfig" standardize="true" phoneticize="true">            <config>                <block-definition  number="FamilyNameGivenNameDOB">                    <hint></hint>                    <block-rule>                        <equals>                            <field>Enterprise.SystemSBR.UKPatient.Name.FamilyName_Phon</field>                            <source>UKPatient.Name.FamilyName_Phon</source>                        </equals>                        <equals>                            <field>Enterprise.SystemSBR.UKPatient.Name.GivenName_Phon</field>                            <source>UKPatient.Name.GivenName_Phon</source>                        </equals>                        <equals>                            <field>Enterprise.SystemSBR.UKPatient.DOB</field>                            <source>UKPatient.DOB</source>                        </equals>                    </block-rule>                </block-definition>                <block-definition  number="FamilyNameDOBGender">                    <hint></hint>                    <block-rule>                        <equals>                            <field>Enterprise.SystemSBR.UKPatient.Name.GivenName</field>                            <source>UKPatient.Name.GivenName_Phon</source>                        </equals>                        <equals>                            <field>Enterprise.SystemSBR.UKPatient.Gender</field>                            <source>UKPatient.Gender</source>                        </equals>                        <equals>                            <field>Enterprise.SystemSBR.UKPatient.DOB</field>                            <source>UKPatient.DOB</source>                        </equals>                    </block-rule>                </block-definition>                <block-definition  number="DOBPostcode">                    <hint></hint>                    <block-rule>                        <equals>                            <field>Enterprise.SystemSBR.UKPatient.DOB</field>                            <source>UKPatient.DOB</source>                        </equals>                        <equals>                            <field>Enterprise.SystemSBR.UKPatient.Address.Postcode</field>                            <source>UKPatient.Address.Postcode</source>                        </equals>                    </block-rule>                </block-definition>                <block-definition  number="HealthID">                    <hint></hint>                    <block-rule>                        <equals>                            <field>Enterprise.SystemSBR.UKPatient.HealthID.IdNum</field>                            <source>UKPatient.HealthID.IdNum</source>                        </equals>                        <equals>                            <field>Enterprise.SystemSBR.UKPatient.HealthID.IdNumType</field>                            <source>UKPatient.HealthID.IdNumType</source>                        </equals>                    </block-rule>                </block-definition>            </config>        </query-builder>    </QueryBuilderConfig></Configuration>