        <SecurityPlugInClass></SecurityPlugInClass>
        <ejbSecurity>ON</ejbSecurity>
        <role>
            <role-name>MasterIndex.User</role-name>
            <operations>
                <name>lookupAssumedMatches</name>
                <name>lookupAuditLog</name>
                <name>lookupPotentialDuplicates</name>
                <name>lookupSystemDefinition</name>
                <name>lookupSystemDefinitions</name>
                <name>lookupSystemObjectPKs</name>
                <name>lookupSystemObjects</name>
                <name>lookupTransactions</name>
                <name>resolvePotentialDuplicate</name>
