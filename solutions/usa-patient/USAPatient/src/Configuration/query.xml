<?xml version="1.0" encoding="UTF-8"?><Configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schema/query.xsd">    <QueryBuilderConfig module-name="QueryBuilder" parser-class="com.sun.mdm.index.configurator.impl.querybuilder.QueryBuilderConfiguration">        <query-builder name="ALPHA-SEARCH" class="com.sun.mdm.index.querybuilder.BasicQueryBuilder" parser-class="com.sun.mdm.index.configurator.impl.querybuilder.KeyValueConfiguration" standardize="false" phoneticize="false">            <config>                <option key="UseWildcard" value="true"/>            </config>        </query-builder>        <query-builder name="PHONETIC-SEARCH" class="com.sun.mdm.index.querybuilder.BasicQueryBuilder" parser-class="com.sun.mdm.index.configurator.impl.querybuilder.KeyValueConfiguration" standardize="true" phoneticize="true">            <config>                <option key="UseWildcard" value="false"/>            </config>        </query-builder>        <query-builder name="BLOCKER-SEARCH" class="com.sun.mdm.index.querybuilder.BlockerQueryBuilder" parser-class="com.sun.mdm.index.configurator.impl.blocker.BlockerConfig" standardize="true" phoneticize="true">            <config>                <block-definition  number="FirstNameLastName">                    <hint></hint>                    <block-rule>                        <equals>                            <field>Enterprise.SystemSBR.USAPatient.FirstName_Phon</field>                            <source>USAPatient.FirstName_Phon</source>                        </equals>                        <equals>                            <field>Enterprise.SystemSBR.USAPatient.LastName_Phon</field>                            <source>USAPatient.LastName_Phon</source>                        </equals>                    </block-rule>                </block-definition>                <block-definition  number="SSN">                    <hint></hint>                    <block-rule>                        <equals>                            <field>Enterprise.SystemSBR.USAPatient.SSN</field>                            <source>USAPatient.SSN</source>                        </equals>                    </block-rule>                </block-definition>                <block-definition  number="FirstNameDOBGender">                    <hint></hint>                    <block-rule>                        <equals>                            <field>Enterprise.SystemSBR.USAPatient.FirstName_Phon</field>                            <source>USAPatient.FirstName_Phon</source>                        </equals>                        <equals>                            <field>Enterprise.SystemSBR.USAPatient.DOB</field>                            <source>USAPatient.DOB</source>                        </equals>                        <equals>                            <field>Enterprise.SystemSBR.USAPatient.Gender</field>                            <source>USAPatient.Gender</source>                        </equals>                    </block-rule>                </block-definition>            </config>        </query-builder>    </QueryBuilderConfig></Configuration>