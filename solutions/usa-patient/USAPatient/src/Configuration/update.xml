<?xml version="1.0" encoding="UTF-8"?><Configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schema/update.xsd">    <SurvivorHelperConfig module-name="SurvivorHelper" parser-class="com.sun.mdm.index.configurator.impl.SurvivorHelperConfig">        <helper-class>com.sun.mdm.index.survivor.impl.DefaultSurvivorHelper</helper-class>        <default-survivor-strategy>            <strategy-class>com.sun.mdm.index.survivor.impl.WeightedSurvivorStrategy</strategy-class>            <parameters>                <parameter>                    <parameter-name>ConfigurationModuleName</parameter-name>                    <parameter-type>java.lang.String</parameter-type>                    <parameter-value>WeightedSurvivorCalculator</parameter-value>                </parameter>            </parameters>        </default-survivor-strategy>        <candidate-definitions>            <candidate-field name="USAPatient.FirstName"/>            <candidate-field name="USAPatient.FirstName_Std"/>            <candidate-field name="USAPatient.FirstName_Phon"/>            <candidate-field name="USAPatient.MiddleName"/>            <candidate-field name="USAPatient.LastName"/>            <candidate-field name="USAPatient.LastName_Std"/>            <candidate-field name="USAPatient.LastName_Phon"/>            <candidate-field name="USAPatient.Suffix"/>            <candidate-field name="USAPatient.Title"/>            <candidate-field name="USAPatient.SSN"/>            <candidate-field name="USAPatient.DOB"/>            <candidate-field name="USAPatient.Death"/>            <candidate-field name="USAPatient.Gender"/>            <candidate-field name="USAPatient.MStatus"/>            <candidate-field name="USAPatient.Race"/>            <candidate-field name="USAPatient.Ethnic"/>            <candidate-field name="USAPatient.Religion"/>            <candidate-field name="USAPatient.Language"/>            <candidate-field name="USAPatient.SpouseName"/>            <candidate-field name="USAPatient.MotherName"/>            <candidate-field name="USAPatient.MotherMN"/>            <candidate-field name="USAPatient.FatherName"/>            <candidate-field name="USAPatient.Maiden"/>            <candidate-field name="USAPatient.PobCity"/>            <candidate-field name="USAPatient.PobState"/>            <candidate-field name="USAPatient.PobCountry"/>            <candidate-field name="USAPatient.VIPFlag"/>            <candidate-field name="USAPatient.VetStatus"/>            <candidate-field name="USAPatient.Status"/>            <candidate-field name="USAPatient.DriverLicense"/>            <candidate-field name="USAPatient.DriverLicenseSt"/>            <candidate-field name="USAPatient.Dod"/>            <candidate-field name="USAPatient.DeathCertificate"/>            <candidate-field name="USAPatient.Nationality"/>            <candidate-field name="USAPatient.Citizenship"/>            <candidate-field name="USAPatient.RepatriationNo"/>            <candidate-field name="USAPatient.MilitaryBranch"/>            <candidate-field name="USAPatient.MilitaryRank"/>            <candidate-field name="USAPatient.MilitaryStatus"/>            <candidate-field name="USAPatient.DistrictOfResidence"/>            <candidate-field name="USAPatient.Alias[*].*"/>            <candidate-field name="USAPatient.Address[*].*"/>            <candidate-field name="USAPatient.Phone[*].*"/>            <candidate-field name="USAPatient.ExistingCon[*].*"/>            <candidate-field name="USAPatient.Contact[*].*"/>            <candidate-field name="USAPatient.Insurance[*].*"/>            <candidate-field name="USAPatient.Comment[*].*"/>            <candidate-field name="USAPatient.AuxId[*].*"/>        </candidate-definitions>    </SurvivorHelperConfig>    <WeightedCalculator module-name="WeightedSurvivorCalculator" parser-class="com.sun.mdm.index.configurator.impl.WeightedCalculatorConfig">        <default-parameters>            <parameter>                <quality>SourceSystem</quality>                <preference>SUN</preference>                <utility>100.0</utility>            </parameter>            <parameter>                <quality>MostRecentModified</quality>                <utility>75.0</utility>            </parameter>        </default-parameters>    </WeightedCalculator>    <UpdateManagerConfig module-name="UpdateManager" parser-class="com.sun.mdm.index.configurator.impl.UpdateManagerConfig">        <EnterpriseMergePolicy></EnterpriseMergePolicy>        <EnterpriseUnmergePolicy></EnterpriseUnmergePolicy>        <EnterpriseUpdatePolicy></EnterpriseUpdatePolicy>        <EnterpriseCreatePolicy></EnterpriseCreatePolicy>        <SystemMergePolicy></SystemMergePolicy>        <SystemUnmergePolicy></SystemUnmergePolicy>        <UndoAssumeMatchPolicy></UndoAssumeMatchPolicy>        <SkipUpdateIfNoChange>true</SkipUpdateIfNoChange>    </UpdateManagerConfig></Configuration>