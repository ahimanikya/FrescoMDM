<?xml version="1.0" encoding="UTF-8"?><Configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schema/update.xsd">    <SurvivorHelperConfig module-name="SurvivorHelper" parser-class="com.sun.mdm.index.configurator.impl.SurvivorHelperConfig">        <helper-class>com.sun.mdm.index.survivor.impl.DefaultSurvivorHelper</helper-class>        <default-survivor-strategy>            <strategy-class>com.sun.mdm.index.survivor.impl.WeightedSurvivorStrategy</strategy-class>            <parameters>                <parameter>                    <parameter-name>ConfigurationModuleName</parameter-name>                    <parameter-type>java.lang.String</parameter-type>                    <parameter-value>WeightedSurvivorCalculator</parameter-value>                </parameter>            </parameters>        </default-survivor-strategy>        <candidate-definitions>            <candidate-field name="AUSPatient.Category"/>            <candidate-field name="AUSPatient.FirstName"/>            <candidate-field name="AUSPatient.FirstName_Std"/>            <candidate-field name="AUSPatient.FirstName_Phon"/>            <candidate-field name="AUSPatient.MiddleName"/>            <candidate-field name="AUSPatient.MiddleName_Std"/>            <candidate-field name="AUSPatient.MiddleName_Phon"/>            <candidate-field name="AUSPatient.LastName"/>            <candidate-field name="AUSPatient.LastName_Std"/>            <candidate-field name="AUSPatient.LastName_Phon"/>            <candidate-field name="AUSPatient.Suffix"/>            <candidate-field name="AUSPatient.Title"/>            <candidate-field name="AUSPatient.MedicareNo"/>            <candidate-field name="AUSPatient.DOB"/>            <candidate-field name="AUSPatient.Death"/>            <candidate-field name="AUSPatient.Gender"/>            <candidate-field name="AUSPatient.MStatus"/>            <candidate-field name="AUSPatient.IndigStatus"/>            <candidate-field name="AUSPatient.Religion"/>            <candidate-field name="AUSPatient.Language"/>            <candidate-field name="AUSPatient.Maiden"/>            <candidate-field name="AUSPatient.PobCountry"/>            <candidate-field name="AUSPatient.DVAColour"/>            <candidate-field name="AUSPatient.Status"/>            <candidate-field name="AUSPatient.Dod"/>            <candidate-field name="AUSPatient.DeathCertificate"/>            <candidate-field name="AUSPatient.RepatriationNo"/>            <candidate-field name="AUSPatient.PensionNo"/>            <candidate-field name="AUSPatient.PensionExpDate"/>            <candidate-field name="AUSPatient.LgaCode"/>            <candidate-field name="AUSPatient.Alias[*].*"/>            <candidate-field name="AUSPatient.Address[*].*"/>            <candidate-field name="AUSPatient.Phone[*].*"/>            <candidate-field name="AUSPatient.ExistingCon[*].*"/>            <candidate-field name="AUSPatient.Contact[*].*"/>            <candidate-field name="AUSPatient.HeathFund[*].*"/>            <candidate-field name="AUSPatient.Comment[*].*"/>            <candidate-field name="AUSPatient.AuxId[*].*"/>        </candidate-definitions>    </SurvivorHelperConfig>    <WeightedCalculator module-name="WeightedSurvivorCalculator" parser-class="com.sun.mdm.index.configurator.impl.WeightedCalculatorConfig">        <default-parameters>            <parameter>                <quality>SourceSystem</quality>                <preference>SUN</preference>                <utility>100.0</utility>            </parameter>            <parameter>                <quality>MostRecentModified</quality>                <utility>75.0</utility>            </parameter>        </default-parameters>    </WeightedCalculator>    <UpdateManagerConfig module-name="UpdateManager" parser-class="com.sun.mdm.index.configurator.impl.UpdateManagerConfig">        <EnterpriseMergePolicy></EnterpriseMergePolicy>        <EnterpriseUnmergePolicy></EnterpriseUnmergePolicy>        <EnterpriseUpdatePolicy></EnterpriseUpdatePolicy>        <EnterpriseCreatePolicy></EnterpriseCreatePolicy>        <SystemMergePolicy></SystemMergePolicy>        <SystemUnmergePolicy></SystemUnmergePolicy>        <UndoAssumeMatchPolicy></UndoAssumeMatchPolicy>        <SkipUpdateIfNoChange>true</SkipUpdateIfNoChange>    </UpdateManagerConfig></Configuration>