<?xml version="1.0" encoding="UTF-8"?>
<!-- NOTE:  This is a sample file.  It contains the basic format for the security -->
<!-- configuration file (midm-security.xml)                                               -->
<midm-security xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schema/midm-security.xsd">
    <roles>
        <role>
            <role-name>Administrator</role-name>
            <operations>
                <name>AuditLog_SearchView</name>
                <name>AuditLog_Print</name>
                <name>EO_Deactivate</name>
                <name>EO_SearchViewSBR</name>
                <name>EO_Compare</name>
                <name>EO_PrintSBR</name>
                <name>EO_Activate</name>
                <name>EO_Edit</name>
                <name>EO_Unmerge</name>
                <name>EO_PrintComparison</name>
                <name>EO_Merge</name>
                <name>EO_Create</name>
                <name>EO_ViewMergeTree</name>
                <name>EO_OverwriteSBR</name>
                <name>EO_LinkSBRFields</name>
                <name>EO_UnlinkSBRFields</name>
                <name>EO_LockSBRFields</name>
                <name>EO_UnlockSBRFields</name>
                <name>AssumedMatch_SearchView</name>
                <name>AssumedMatch_Undo</name>
                <name>AssumedMatch_Print</name>
                <name>TransLog_SearchView</name>
                <name>TransLog_Print</name>
                <name>SO_Remove</name>
                <name>SO_SearchView</name>
                <name>SO_Edit</name>
                <name>SO_Compare</name>
                <name>SO_Add</name>
                <name>SO_Unmerge</name>
                <name>SO_Merge</name>
                <name>SO_Print</name>
                <name>SO_Deactivate</name>
                <name>SO_Activate</name>
                <name>PotDup_SearchView</name>
                <name>PotDup_Print</name>
                <name>PotDup_Unresolve</name>
                <name>PotDup_ResolveUntilRecalc</name>
                <name>PotDup_ResolvePermanently</name>
                <name>Reports_View</name>
                <name>Reports_MergedRecords</name>
                <name>Reports_DeactivatedEUIDs</name>
                <name>Reports_UnmergedRecords</name>
                <name>Reports_Updates</name>
                <name>Reports_Activity</name>
                <name>Reports_Duplicates</name>
                <name>Reports_AssumedMatches</name>
                <name>Field_VIP</name>
            </operations>
        </role>
        <role>
            <role-name>Clerk</role-name>
            <operations>
                <name>EO_SearchViewSBR</name>
                <name>EO_Compare</name>
                <name>EO_PrintSBR</name>
                <name>EO_PrintComparison</name>
                <name>EO_ViewMergeTree</name>
                <name>TransLog_All</name>
                <name>SO_SearchView</name>
                <name>SO_Print</name>
                <name>PotDup_SearchView</name>
                <name>PotDup_Print</name> 
            </operations>
        </role>
        <role>
            <role-name>DataEntry</role-name>
            <inheritance>
                <inherits-from>Clerk</inherits-from>
                <excluded-operations>
                    <name>EO_ViewMergeTree</name>
                    <name>TransLog_All</name>
                </excluded-operations>
            </inheritance>
            <operations>
                <name>Reports_MergedRecords</name>
                <name>Reports_DeactivatedEUIDs</name>
            </operations>
        </role>
        <role>
            <role-name>DataEntry2</role-name>
            <inheritance>
                <inherits-from>Clerk</inherits-from>
                <excluded-operations>
                    <name>EO_ViewMergeTree</name>
                    <name>TransLog_All</name>
                </excluded-operations>
            </inheritance>
            <inheritance>
                <inherits-from>DataEntry</inherits-from>
                <excluded-operations>
                    <name>Reports_MergedRecords</name>
                    <name>Reports_DeactivatedEUIDs</name>
                </excluded-operations>
            </inheritance>
            <operations>
                <name>EO_Add</name>
            </operations>
        </role>
    </roles>
</midm-security>
