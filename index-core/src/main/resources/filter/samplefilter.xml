<?xml version="1.0" encoding="ISO-8859-1"?>
<filter  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="filter.xsd">
    <exclusion-List module-name="ExclusionFilter" parser-class="com.stc.eindex.filter.ExclusionFilterCofig" >
        <field sbr="true" matching="false" blocking="false">
            <name>PatientView.FirstName</name>
            <value>
                <field-value>baby </field-value>
                <field-value>boby </field-value>
            </value>
        </field>
        <field sbr="true" matching="true" blocking="false">
            <name>PatientView.LastName</name>
            <value>
                <field-value> roy </field-value>
                <field-value>joy </field-value>
            </value>
        </field>
        <field sbr="false" matching="false" blocking="true">
            <name>PatientView.LastName</name>
            <value>
                <field-value>aaaa </field-value>
                <field-value> bbbb </field-value>
                </value>
        </field>
        <field sbr="true" matching="true" blocking="true">
            <name>PatientView.SSN</name>
            <value>
                <file delimiter="|"> 
                    <file-name> exclusionSSN.txt  </file-name> 
	        </file>
            </value>
        </field>
        <field sbr="false" matching="true" blocking="false">
            <name>PatientView.SSN</name>
            <value>
                <file delimiter="|"> 
                    <file-name>matching.txt </file-name> 
	        </file>
            </value>
        </field>  
        <field sbr="false" matching="true" blocking="true">
            <name>PatientView.Age</name>
            <value>
                <field-value> 999 </field-value>
                <field-value>000 </field-value>
                <field-value>777 </field-value>
            </value>
        </field> 
    </exclusion-List>
</filter>
