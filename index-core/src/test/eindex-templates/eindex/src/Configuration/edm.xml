			<value-type>string</value-type>
			<input-mask>(DDD) DDD-DDDD</input-mask>
			<value-mask>xDDDxxDDDxDDDD</value-mask>
                        <is-sensitive>true</is-sensitive>
		</field-Phone>
		<field-PhoneExt>
			<display-name>Extension</display-name>
			<display-order>3</display-order>
			<max-length>6</max-length>
			<gui-type>TextBox</gui-type>
			<value-type>string</value-type>
                        <is-sensitive>true</is-sensitive>
		</field-PhoneExt>
	</node-Phone>
	<node-AuxId display-order="4">
		<field-AuxIdDef>
			<display-name>Auxiliary ID Type</display-name>
			<display-order>1</display-order>
			<max-length>10</max-length>
			<gui-type>MenuList</gui-type>
			<value-list>AUXIDDEF</value-list>
			<value-type>string</value-type>
			<key-type>true</key-type>
		</field-AuxIdDef>
		<field-Id>
			<display-name>Auxiliary ID</display-name>
			<display-order>2</display-order>
			<max-length>40</max-length>
			<gui-type>TextBox</gui-type>
			<value-type>string</value-type>
			<key-type>false</key-type>
		</field-Id>
	</node-AuxId>
	<node-Comment display-order="5">
		<field-CommentCode>
			<display-name>Comment Code</display-name>
			<display-order>0</display-order>
			<max-length>8</max-length>
			<gui-type>TextBox</gui-type>
			<value-type>string</value-type>
			<key-type>true</key-type>
		</field-CommentCode>
		<field-EnterDate>
			<display-name>Enter Date</display-name>
			<display-order>1</display-order>
			<max-length>32</max-length>
			<gui-type>TextBox</gui-type>
			<value-type>date</value-type>
		</field-EnterDate>
		<field-CommentText>
			<display-name>Comment Text</display-name>
			<display-order>2</display-order>
			<max-length>1000</max-length>
			<gui-type>TextArea</gui-type>
			<value-type>string</value-type>
